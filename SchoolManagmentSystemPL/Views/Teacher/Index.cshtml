@model List<TeacherVM>

<h1 class="p-5 text-center box-shadow">All Teachers</h1>



<table class="table table-bordered table-hover w-100 m-auto mb-5 ">
    <thead>
    <td>Name</td>
    <td>Email</td>
    <td>Phone</td>
    <td>Address</td>
    <td>Date Of Birth</td>
    <td>Age</td>
    <td>Gender</td>
    <td>Hire Date</td>
    <td>Subject</td>
    <td>Edit</td>
    <td>Delete</td
    

    </thead>

    <tbody>
        <partial name="_TeacherTable" model="@Model"></partial>
    </tbody>
</table>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}

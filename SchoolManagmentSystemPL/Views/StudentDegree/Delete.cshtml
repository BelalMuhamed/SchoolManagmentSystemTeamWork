@model SchoolManagmentSystem.DAL.Models.StudentDegree

@{
    ViewData["Title"] = "Delete Student Degree";
}

<h2>Are you sure you want to delete this degree?</h2>

<form asp-action="Delete">
    <input type="hidden" asp-for="StudentId" />
    <input type="hidden" asp-for="SubjectId" />
    <input type="hidden" asp-for="ClassId" />
    <input type="hidden" asp-for="TeacherId" />

    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>

